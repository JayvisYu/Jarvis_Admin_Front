if (res.data.msg === 'success') {\n\t\t\t\t\tthis.$message({\n\t\t\t\t\t\tmessage: '获取数据成功',\n\t\t\t\t\t\ttype: 'success',\n\t\t\t\t\t\tshowClose: true,\n\t\t\t\t\t\tduration: 1000\n\t\t\t\t\t})\n\t\t\t\t\tthis.list = res.data.article_list\n\t\t\t\t\tthis.total = res.data.total\n\t\t\t\t\tthis.listLoading = false\n\t\t\t\t} else {\n\t\t\t\t\tthis.$message({\n\t\t\t\t\t\tmessage: '获取数据失败',\n\t\t\t\t\t\ttype: 'error',\n\t\t\t\t\t\tshowClose: true,\n\t\t\t\t\t\tduration: 1000\n\t\t\t\t\t})\n\t\t\t\t\tthis.listLoading = false\n\t\t\t\t}\n\t\t\t}).catch(err => {\n\t\t\t\tconsole.log(err);\n\t\t\t})\n\t\t}\n\t}\n}\n</script>\n\n<style scoped>\n.edit-input {\n\tpadding-right: 100px;\n}\n.cancel-btn {\n\tposition: absolute;\n\tright: 15px;\n\ttop: 10px;\n}\n</style>\n"],"sourceRoot":"src/views/article"}]}